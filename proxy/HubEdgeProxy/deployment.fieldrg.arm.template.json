{
  "$schema-template": "2.0.0",
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "bineiot": {
                "username": "$CONTAINER_REGISTRY_USERNAME_acrbineiotdevsp",
                "password": "$CONTAINER_REGISTRY_PASSWORD_acrbineiotdevsp",
                "address": "acrbineiotdev.azurecr.io"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "env": {
              "storageFolder": {
                  "value": "/iotedge/storage/"
              }
            }, 
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.1.1",
              "createOptions": {
                "HostConfig": {
                  "Binds":["/opt/data/settings/iotedge/storageonhost:/iotedge/storage/"]
                }
              }
            }
          },
          "edgeHub": {
            "type": "docker",
            "env": {
              "storageFolder": {
                  "value": "/iotedge/storage/"
              }
            },
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.1.1",
              "createOptions": {
                "HostConfig": {
                  "Binds":["/opt/data/settings/iotedge/storageonhost:/iotedge/storage/"],
                  "PortBindings": {
                    "5671/tcp": [
                      {
                        "HostPort": "5671"
                      }
                    ],
                    "8883/tcp": [
                      {
                        "HostPort": "8883"
                      }
                    ],
                    "443/tcp": [
                      {
                        "HostPort": "443"
                      }
                    ]
                  }
                }
              }
            }
          }
        },
        "modules": {
          "HubEdgeProxyModule": {
            "version": "1.0",
            "type": "docker",
            "env": {
              "storageFolder": {
                  "value": "/app/logs/"
              }
            },
            "status": "running",
            "restartPolicy": "always",
            "startupOrder": 0,
            "settings": {
              "image": "acrbineiotdev.azurecr.io/hubedgeproxymodule:arm32v7-20210726.3",
              "createOptions": {
                "HostConfig": {
                  "Binds":["/opt/data/settings/iotedge/hubedgeproxymoduleonhost/logs:/app/logs/"],
                  "PortBindings": {
                    "5001/tcp": [
                      {
                        "HostPort": "5001"
                      }
                    ]
                  }
                }
              }
            }
          },
          "HubModule": {
            "version": "1.0",
            "type": "docker",
            "env": {
              "storageFolder": {
                  "value": "/root/hubmoduleonhost"
              },
              "mstore": {
                "value": "/mstore/storage/"
              }
            },
            "status": "running",
            "restartPolicy": "always",
            "startupOrder": 1,
            "settings": {
              "image": "acrbineiotdev.azurecr.io/hubmodule:arm32v7-20210803.1",
              "createOptions": {
                "HostConfig": {
                  "Binds":["/opt/data/settings/iotedge/hubmoduleonhost:/root/hubmoduleonhost/",
                          "/mnt/hdd_1/mstore/QCAST.ipts/storage:/mstore/storage/"],
                  "ExtraHosts":["host.docker.internal:172.17.0.1"],
                  "PortBindings": {
                    "5000/tcp": [
                      {
                        "HostPort": "5000"
                      }
                    ],
                    "5002/tcp": [
                      {
                        "HostPort": "5002"
                      }
                    ]
                  }
                }
              }
            }
          },
          "vodeproxy": {
            "version": "1.0.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "acrbineiotdev.azurecr.io/vodeproxy:1.0.0-arm32v7",
              "createOptions": {
                "HostConfig":{
                  "ExtraHosts":["host.docker.internal:172.17.0.1"]
                }
              }
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "HubEdgeProxyModuleToIoTHub": "FROM /messages/modules/HubEdgeProxyModule/outputs/* INTO $upstream",
          "HubModuleToIoTHub": "FROM /messages/modules/HubModule/outputs/* INTO $upstream",
          "vodeproxyToIoTHub": "FROM /messages/modules/vodeproxy/outputs/* INTO $upstream"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 172800
        }
      }
    },
    "HubEdgeProxyModule": {
      "properties.desired": {
        "ProxyModuleClientEndpoints":
        {
          "HUBServer":"http://HubModule:5002"
        }
      }
    }
  }
}